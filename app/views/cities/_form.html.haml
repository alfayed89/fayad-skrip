= form_for @city do |f|
  - if @city.errors.any?
    #error_explanation
      %h2= "#{pluralize(@city.errors.count, "error")} prohibited this city from being saved:"
      %ul
        - @city.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nama
    = f.text_field :nama
  .field
    = f.label :province_id
    = f.collection_select :province_id, Province.all, :id, :nama
  .actions
    = f.submit 'Save'
